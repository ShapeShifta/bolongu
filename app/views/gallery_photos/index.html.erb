<h1>Listing gallery_photos</h1>

<table>
  <tr>
    <th>String</th>
    <th>Caption</th>
    <th>Gallery</th>
  </tr>

<% @gallery_photos.each do |gallery_photo| %>
  <tr>
    <td><%=h gallery_photo.string %></td>
    <td><%=h gallery_photo.caption %></td>
    <td><%=h gallery_photo.gallery %></td>
    <td><%= link_to 'Show', gallery_photo %></td>
    <td><%= link_to 'Edit', edit_gallery_photo_path(gallery_photo) %></td>
    <td><%= link_to 'Destroy', gallery_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New gallery_photo', new_gallery_photo_path %>